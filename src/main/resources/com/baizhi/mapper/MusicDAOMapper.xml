<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.baizhi.daos.MusicDao">
    <insert id="insertmusic" parameterType="Music">
        insert into music (id,title,filesize,path,uploadtime,url,pic,album_id)
        values (#{id},#{title},#{filesize},#{path},#{uploadtime},#{url},#{pic},#{album_id})
    </insert>

</mapper>

















